  "solargraph.hover": true,
  "solargraph.useBundler": false,
  "solargraph.trace.server": "verbose",
  "suggest.enablePreview": true,
  "markdownlint.config": {
    "default": true,
    "line_length": false
  },
  "coc.preferences.enableMarkdown": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.presets": {
    "panel": {
      "sources": [
        {
          "name": "buffer",
          "expand": false
        },
        {
          "name": "file",
          "expand": true
        }
      ],
      "width": 40
    },
    "floating": {
      "sources": [
        {
          "name": "buffer",
          "expand": false
        },
        {
          "name": "file",
          "expand": true
        }
      ],
      "position": "floating",
      "floating-position": "center",
      "floating-width": 80
    },
    "simplify": {
      "file.child.template": "[selection | clip | 1] [indent][icon | 1] [filename omitCenter 1]"
    }
  },
  "explorer.file.autoReveal": true,
  "go.trace.server": "verbose",
  "go.goplsArgs": ["-rpc.trace"],
  "go.goplsPath": "/home/doohee/go/bin/gopls",
